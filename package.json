    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-awesome-styled-grid": "^3.0.10",
    "react-dom": "^16.13.1",
    "react-query": "^2.4.11",
    "react-scripts": "3.4.1",
    "react-spinners-kit": "^1.9.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
